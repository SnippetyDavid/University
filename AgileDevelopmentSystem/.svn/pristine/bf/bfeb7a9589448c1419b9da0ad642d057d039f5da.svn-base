<Window x:Class="EasyWpfLoginNavigateExample.View.AddUserStoryToProductBaclog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddUserStoryToProductBaclog" Height="373.8" Width="408">
    <Grid Margin="0,-21,0,-59">
        <TextBox Text="{Binding Stories.userStory}" HorizontalAlignment="Left" Height="23" Margin="170,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Text="{Binding Stories.marketValue}" HorizontalAlignment="Left" Height="23" Margin="170,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Text="{Binding Stories.storyPoints}" HorizontalAlignment="Left" Height="23" Margin="170,152,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Command="{Binding AddUserStoriesCommand}" Content="Create User story" HorizontalAlignment="Left" Margin="170,223,0,0" VerticalAlignment="Top" Width="107" Height="33"/>
        <Label Content="User Story" HorizontalAlignment="Left" Margin="55,58,0,0" VerticalAlignment="Top" Width="95"/>
        <Label Content="Market Value" HorizontalAlignment="Left" Margin="55,101,0,0" VerticalAlignment="Top" Width="95"/>
        <Label Content="Story Points" HorizontalAlignment="Left" Margin="55,148,0,0" VerticalAlignment="Top" Width="95"/>

    </Grid>
</Window>
