<Window x:Class="EasyWpfLoginNavigateExample.View.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window3" Height="600" Width="950" WindowState="Maximized" WindowStyle="None" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="21*"/>
            <RowDefinition Height="179*"/>
        </Grid.RowDefinitions>
        <Menu HorizontalAlignment="Left" Height="537" VerticalAlignment="Top" Width="145" Grid.Row="1" Grid.ColumnSpan="2">
            <Menu Height="211" Width="99"/>
        </Menu>
        <ComboBox x:Name="Projects" HorizontalAlignment="Left" Margin="0,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="120" ItemsSource="{Binding MyItems}" SelectedItem="{Binding MySelectedItem}" SelectionChanged="Projects" Grid.Column="1" Height="22"/>
        <ComboBox x:Name="PlanningPoker" HorizontalAlignment="Left" Margin="0,65,0,0" Grid.Row="1" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged_1" Grid.Column="1" Height="22"/>
        <ComboBox HorizontalAlignment="Left" Margin="0,123,0,0" Grid.Row="1" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged_1" Grid.Column="1" Height="22"/>
        <TextBox x:Name="SearchBox" HorizontalAlignment="Left" Height="22" Margin="0,505,0,0" Grid.Row="1" TextWrapping="Wrap" Text="SearchBox" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
        <Grid HorizontalAlignment="Left" Height="100" Grid.RowSpan="2" VerticalAlignment="Top" Width="100" Grid.ColumnSpan="2"/>
        <Grid HorizontalAlignment="Left" Height="537" Margin="80,0,0,0" Grid.Row="1" VerticalAlignment="Top" Width="805" Grid.Column="1"/>
        <DataGrid HorizontalAlignment="Left" Height="5" Margin="780,401,0,0" Grid.Row="1" VerticalAlignment="Top" Width="3" Grid.Column="1"/>
        <Image HorizontalAlignment="Left" Height="50" Margin="415,10,0,0" VerticalAlignment="Top" Width="50" Grid.Column="1"/>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="941*"/>
        </Grid.ColumnDefinitions>
    </Grid>
</Window>
