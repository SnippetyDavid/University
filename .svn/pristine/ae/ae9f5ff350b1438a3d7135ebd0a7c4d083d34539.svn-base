<Window x:Class="EasyWpfLoginNavigateExample.View.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window4" Height="600" Width="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="36*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="354*"/>
            <RowDefinition Height="156*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="149*"/>
            <ColumnDefinition Width="843*"/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="LoggedInPic" HorizontalAlignment="Left" Height="60" VerticalAlignment="Top" Width="165" Background="#FF66CCFF" Grid.ColumnSpan="3" Grid.RowSpan="3">
            <Grid x:Name="Content" HorizontalAlignment="Left" Height="512" Margin="164,61,-827,-513" VerticalAlignment="Top" Width="828" Background="#FFF2F2F2"/>
            <Label x:Name="Title" Content="Scrum Mangement" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,16,5,0" Height="28" Width="130" FontSize="13" FontFamily="Century Gothic" Foreground="White" FontWeight="Bold"/>
            <Image x:Name="Logo" HorizontalAlignment="Left" Margin="6,16,0,20" Width="24" Source="Images/smlogo.png"/>
        </Grid>
        <Grid x:Name="TopPanel" HorizontalAlignment="Left" Height="60" Margin="21,0,0,0" VerticalAlignment="Top" Width="822" Background="White" Grid.Column="2" Grid.RowSpan="3">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="0*"/>
            </Grid.ColumnDefinitions>
        </Grid>
        <Grid HorizontalAlignment="Left" Height="509" Margin="0,1,0,0" VerticalAlignment="Top" Width="165" Background="#FF99CCFF" Grid.ColumnSpan="3" Grid.Row="2" Grid.RowSpan="2"/>
    </Grid>
</Window>
